<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
	</settings>
	
	<typeAliases>
		<typeAlias alias="RoleRight" type="org.linys.model.system.RoleRight" />
		<typeAlias alias="Right" type="org.linys.model.system.Right" />
		<typeAlias alias="Role" type="org.linys.model.system.Role" />
		<typeAlias alias="User" type="org.linys.model.system.User" />
	</typeAliases>

</configuration>