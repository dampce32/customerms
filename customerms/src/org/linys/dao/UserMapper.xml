<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.linys.dao.UserMapper" >
  <insert id="insert" parameterType="org.linys.model.User" >
    insert into t_user (userId, userCode, userName, 
      passwords)
    values (#{userId,jdbcType=INTEGER}, #{userCode,jdbcType=VARCHAR}, #{userName,jdbcType=VARCHAR}, 
      #{passwords,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="org.linys.model.User" >
    insert into t_user
    <trim prefix="(" suffix=")" suffixOverrides="," >
      userId,
      <if test="userCode != null" >
        userCode,
      </if>
      <if test="userName != null" >
        userName,
      </if>
      <if test="passwords != null" >
        passwords,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      #{userId,jdbcType=INTEGER},
      <if test="userCode != null" >
        #{userCode,jdbcType=VARCHAR},
      </if>
      <if test="userName != null" >
        #{userName,jdbcType=VARCHAR},
      </if>
      <if test="passwords != null" >
        #{passwords,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>